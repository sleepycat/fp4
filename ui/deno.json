{
  "tasks": {
    "build": "rsbuild build",
    "check": "biome check --write",
    "dev": "deno task rsbuild && deno task css",
    "rsbuild": "rsbuild dev",
    "css": "panda --watch",
    "format": "biome format --write",
    "preview": "rsbuild preview",
    "extract": "lingui extract --clean",
    "compile": "lingui compile",
    "panda:init": "panda init --postcss"
  },
  "imports": {
    "@std/expect": "jsr:@std/expect@^1.0.17",
    "@std/testing": "jsr:@std/testing@^1.0.16",
    "@lingui/babel-plugin-lingui-macro": "npm:@lingui/babel-plugin-lingui-macro@^5.6.0",
    "@lingui/core": "npm:@lingui/core@^5.6.0",
    "@lingui/react": "npm:@lingui/react@^5.6.0",
    "@rsbuild/core": "npm:@rsbuild/core@1.5.17",
    "react": "npm:react@19.1.1",
    "react-aria-components": "npm:react-aria-components@^1.13.0",
    "react-dom": "npm:react-dom@19.1.1",
    "react-error-boundary": "npm:react-error-boundary@^6.0.0",
    "react-router": "npm:react-router@^7.9.6",
    "urql": "npm:urql@^5.0.1",
    "@lingui/cli": "npm:@lingui/cli@^5.6.0",
    "@lingui/swc-plugin": "npm:@lingui/swc-plugin@^5.6.1",
    "@pandacss/dev": "npm:@pandacss/dev@^1.2.0",
    "@rsbuild/plugin-react": "npm:@rsbuild/plugin-react@^1.4.2",
    "@testing-library/dom": "npm:@testing-library/dom@^10.4.1",
    "@testing-library/react": "npm:@testing-library/react@^16.3.0",
    "@types/react": "npm:@types/react@^19.1.12",
    "@types/react-dom": "npm:@types/react-dom@^19.1.9"
  },
  "nodeModulesDir": "auto",
  "fmt": {
    "options": {
      "semiColons": false
    }
  }
}
